{
  "id": null,
  "uid": "obs-realtime",
  "title": "Observability App - Realtime",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1s",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Request rate by status (rps)",
      "datasource": {
        "type": "prometheus",
        "uid": "bf8p6myv71h4wd"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status)(rate(app_http_requests_total{route!~\"/(app\\.js|style\\.css|favicon\\.ico)\"}[15s]))",
          "interval": "1s",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "gridPos": {"h": 9, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error rate (404/500)",
      "datasource": {
        "type": "prometheus",
        "uid": "bf8p6myv71h4wd"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status)(rate(app_http_requests_total{status=~\"404|500\",route!~\"/(app\\.js|style\\.css|favicon\\.ico)\"}[15s]))",
          "interval": "1s",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "gridPos": {"h": 9, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Latency (p50/p90/p99 seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "bf8p6myv71h4wd"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum by (le)(rate(app_http_request_duration_seconds_bucket{route!~\"/(app\\.js|style\\.css|favicon\\.ico)\"}[30s])))",
          "legendFormat": "p50",
          "interval": "1s"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.9, sum by (le)(rate(app_http_request_duration_seconds_bucket{route!~\"/(app\\.js|style\\.css|favicon\\.ico)\"}[30s])))",
          "legendFormat": "p90",
          "interval": "1s"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum by (le)(rate(app_http_request_duration_seconds_bucket{route!~\"/(app\\.js|style\\.css|favicon\\.ico)\"}[30s])))",
          "legendFormat": "p99",
          "interval": "1s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "gridPos": {"h": 9, "w": 12, "x": 0, "y": 9}
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "In-flight requests",
      "datasource": {
        "type": "prometheus",
        "uid": "bf8p6myv71h4wd"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "app_http_in_flight_requests",
          "legendFormat": "in-flight",
          "interval": "1s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "gridPos": {"h": 9, "w": 12, "x": 12, "y": 9}
    }
  ]
}
